Cell[CellGroupData[{

Cell["Ablowitz-Ladik discretization of the NLS equation", "Subsection",
  TextAlignment->Left,
  TextJustification->1,
  CellTags->"Examples"],

Cell[BoxData[{
    \(\(ablowitzladikdde1 := \[PartialD]\_t u[n, t] - \((u[n + 1, t] - 
                2\ u[n, t] + u[n - 1, t])\) - 
            u[n, t]\ v[n, t]\ \((u[n + 1, t] + u[n - 1, t])\) == 
          0;\)\), "\n", 
    \(\(ablowitzladikdde2 := \[PartialD]\_t v[n, t] + \((v[n + 1, t] - 
                2\ v[n, t] + v[n - 1, t])\) + 
            u[n, t]\ v[n, t]\ \((v[n + 1, t] + v[n - 1, t])\) == 
          0;\)\)}], "Input",
  CellLabel->"In[61]:=",
  TextAlignment->Left,
  TextJustification->1,
  CellTags->"Examples"],

